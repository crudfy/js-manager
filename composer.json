{
    "name": "crudfy/js-manager",
    "description": ":package_description",
    "license": "MIT",
    "authors": [
        {
            "name": "Author Name",
            "email": "author@email.com",
            "homepage": "http://author.com"
        }
    ],
    "homepage": "https://github.com/crudfy/js-manager",
    "keywords": ["Laravel", "JsManager"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Crudfy\\JsManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crudfy\\JsManager\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Crudfy\\JsManager\\JsManagerServiceProvider"
            ],
            "aliases": {
                "JsManager": "Crudfy\\JsManager\\Facades\\JsManager"
            }
        }
    }
}
